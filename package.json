{
  "name": "@kappys/react-query-suspense",
  "version": "1.0.0",
  "author": "Alex Marcos <alexsbd1@gmail.com>",
  "description": "Library to avoid render complex items when the state or data is not ready yet.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:tsup": "tsup --dts --minify",
    "changeset": "changeset",
    "clean": "rimraf dist",
    "dev": "pnpm run test:coverage",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src --ext .ts",
    "lint:eslint:fix": "pnpm run lint:eslint --fix",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:prettier:fix": "pnpm run lint:prettier --write",
    "lint:tsc": "tsc --project tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "test:coverage": "pnpm run test --coverage",
    "prepare": "is-ci || husky install",
    "prerelease": "run-p build test:ci",
    "release": "changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kappys1/react-query-suspense.git"
  },
  "bugs": {
    "url": "https://github.com/kappys1/react-query-suspense/issues"
  },
  "lint-staged": {
    "*.ts": [
      "eslint src --ext .ts"
    ]
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@tanstack/react-query": "^4.16.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0",
    "typescript": "*"
  },
  "peerDependencies": {
    "@tanstack/react-query": "*"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "react",
    "react-query",
    "tanstack",
    "tanstack-query",
    "cache",
    "query",
    "query-keys",
    "react-query-suspense",
    "suspense",
    "react-suspense"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
